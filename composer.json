{
  "name": "Harion/Ada",
  "description": "Laravel + CoreUI + Vue Boilerplate",
  "keywords": ["framework", "boilerplate", "laravel", "coreui", "vue"],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Hieu Trinh (Harion)",
      "email": "hariondeveloper@gmail.com",
      "homepage": "https://www.facebook.com/hieuharion",
      "role": "Project Manager, Technical Leader"
    }
  ],
  "require": {
    "php": "^7.0.30",
    "ext-json": "*",
    "fideloper/proxy": "^4.0",
    "fightbulc/moment": "^1.26",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "5.5.*",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "^1.0",
    "lodash-php/lodash-php": "^0.0.6",
    "phpmailer/phpmailer": "~6.0.6",
    "phpoffice/phpspreadsheet": "^1.5",
    "phpoffice/phpword": "^0.17.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "barryvdh/laravel-debugbar": "^3.2",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "routes/utils.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "gitlab-token" : {
      "gitlab.com": "c8tcdYMeGygv4f8eM82S"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
